<h1>Listing skill_relevances</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_relevances.each do |skill_relevance| %>
      <tr>
        <td><%= link_to 'Show', skill_relevance %></td>
        <td><%= link_to 'Edit', edit_skill_relevance_path(skill_relevance) %></td>
        <td><%= link_to 'Destroy', skill_relevance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill relevance', new_skill_relevance_path %>
